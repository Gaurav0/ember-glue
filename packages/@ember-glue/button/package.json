    "eslint-plugin-prettier": "^3.1.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.0.4",
    "qunit-dom": "^1.0.0",
    "stylelint": "^13.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "typescript": "~3.7.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
